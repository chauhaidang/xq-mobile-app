{
  "name": "omitrix-mobile",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "bip": "expo prebuild --no-install --platform ios",
    "bi": "cd ios && pod install && sh ./build-sim.sh && cd ..",
    "bap": "expo prebuild --no-install --platform android",
    "ba": "cd android && ./gradlew :app:assembleRelease && cd .."
  },
  "dependencies": {
    "react-native": "0.74.5",
    "expo": "~51.0.28",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "prettier": "3.3.3"
  },
  "private": true
}
